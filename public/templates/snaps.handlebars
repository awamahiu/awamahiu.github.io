{{#each Snap}}
<div class="snaps">
    <div class="col-md-4 col-sm-6 col-xs-12 single-snap">
        <a a href="#" data-toggle="modal" data-target="#{{title}}">

            <img src="{{imgSrc}}" title="{{title}}" alt="{{imgAlt}}" class="img-responsive">

        </a>
    </div>
</div>

<!-- Modals -->
<div class="modal fade" id="{{title}}" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-body">
                <img src="{{imgSrc}}" title="{{title}}" alt="{{imgAlt}}" class="img-responsive">
            </div>
            <div class="modal-footer">
                <p class="modal-footer-title">{{name}}</p>

                {{#if description}}
                    <p>{{description}}</p>
                {{/if}}

                <br>

                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<!-- #Modals -->
{{/each}}